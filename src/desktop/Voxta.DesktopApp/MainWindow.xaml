<Window x:Class="Voxta.DesktopApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:wv2="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
        xmlns:consoleControl="clr-namespace:ConsoleControl.WPF;assembly=ConsoleControl.WPF"
        Title="Voxta"
        Icon="Resources/voxta.ico"
        Height="600"
        Width="800">
    <Grid Background="Black">
        <wv2:WebView2 x:Name="WebView" NavigationCompleted="WebView_NavigationCompleted" />
        <consoleControl:ConsoleControl x:Name="ConsoleControl" />
    </Grid>
</Window>
